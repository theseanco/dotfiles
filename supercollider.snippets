snippet poemtask
	(
	~${1} = Task({|i,x|
	x=rrand(0,d[${2:\blake}].size);
	inf.do({
	if(i<${3:90},{
		n.sendMsg("/line/${4}",d[${5:\blake}][x][rrand(1,d[${6:\blake}][x].size-1)][0]);
		rrand(${7:2},${8:20}).wait;
		i=i+1;},{
			0.1.wait;
			x=rrand(0,d[${9:\blake}].size);
			i=0;});
		});
	});
	);

