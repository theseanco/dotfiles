snippet wtask
	(
	~${1} = Task({
	inf.do({
	n.sendMsg("/line/${2}",d[\w${3}][rrand(0,d[\w${4}].size)]);
	${5:1}.wait;
	});
	});
	)

snippet iwtask
	(
	~${1} = Task({|i|
	inf.do({
		if(i<${2:90},{
		n.sendMsg("/line/${3}",d[\w${4}][rrand(0,d[\w${5}].size)]);
		${6:1}.wait
		i=i+1;},{
			if(i<${7:300},{
			n.sendMsg("/line/${8}",d[\w${9}][rrand(0,d[\w${10}].size)]);
			${11}.wait;
	i=i+1;},{
	i=0});
	});
	});
	});
	)

